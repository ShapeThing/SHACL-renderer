@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix schema: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dash: <http://datashapes.org/dash#> .
@prefix stsr: <http://ontology.shapething.com/shacl-renderer#> .
@prefix stf: <http://ontology.shapething.com/facet#> .

<#data>
  a schema:Person ;
  schema:icon "line-md:buy-me-a-coffee-twotone"^^<https://iconify.design>, "hugeicons:coffee-beans"^^<https://iconify.design> ;
.

<#with-limit>
  a sh:NodeShape ;
  sh:targetClass schema:Person ;
  sh:closed true ;

  sh:property [
    sh:name "Icon"@en ;
    sh:description "Search for house" ;
    sh:path schema:icon ;
    stsr:collections "fluent-emoji" ;
    sh:datatype <https://iconify.design> ;
  ];
.
